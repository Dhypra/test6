{"ast":null,"code":"import axios from 'axios';\nimport { ref } from 'vue';\nconst modal = ref(false);\nconst edit = ref(false);\nconst del = ref(false);\nexport default {\n  components: {\n    name: 'TappingModule'\n  },\n  data() {\n    return {\n      modal,\n      edit,\n      del,\n      module: [],\n      selectedModuleType: 'All',\n      selectedExporter: 'All',\n      selectedDate: new Date(),\n      page: 0,\n      modalModule: [],\n      modalMod: 0,\n      editIndex: null\n    };\n  },\n  mounted() {\n    axios.get('http://localhost:5000/module').then(response => {\n      console.log(response.data.data);\n      this.module = response.data.data;\n      // this.tmc = response.data.data.filter(\n      //   (module) => module.exporter_name == 'TMC',\n      // )\n      console.log(response.data.data.filter(module => module.exporter_name == 'TMC').length);\n    });\n  },\n  computed: {\n    // Filtered data based on selected filters\n    filteredModule() {\n      return this.module.filter(module => {\n        // Filter by Module Type\n        const matchModuleType = this.selectedModuleType === 'All' || module.module_type === this.selectedModuleType;\n\n        // Filter by Exporter Name\n        console.log(this.selectedExporter);\n        const matchExporter = this.selectedExporter === 'All' || module.exporter_name === this.selectedExporter;\n\n        // Filter by Month Receive\n        let mts = new Date(this.selectedDate);\n        let mtd = new Date(module.date_receive);\n        let mtn = new Date();\n        console.log(mtn);\n        const matchMonth = !this.selectedDate || mtd == mts;\n\n        // Return true if any of the active filters match\n        this.page = 0;\n        return matchModuleType && matchExporter && matchMonth;\n      });\n    }\n  },\n  methods: {\n    // Method untuk menentukan status module\n    getModuleStatus(parts) {\n      console.log(parts);\n      // Periksa jika ada 'null' pada field date_tapping\n      const hasNullTapping = parts.some(part => part.date_tapping === null);\n      return hasNullTapping ? 'Fresh Module' : 'Tapping Module';\n    },\n    handleModal(id) {\n      console.log(id);\n      console.log(this.module.filter(module => module._id == id)[0].module_no);\n      this.modalMod = this.module.filter(module => module._id == id)[0].module_no;\n      this.modalModule = this.module.filter(module => module._id == id)[0].parts;\n      //  console.log(this.module.filter((module) => module._id == id)[0].parts)\n      this.modal = !modal.value;\n      this.edit = false;\n      this.del = false;\n    },\n    handleEdit() {\n      this.edit = !edit.value;\n    },\n    handleDel() {\n      this.del = !del.value;\n    },\n    prevPage() {\n      if (this.page > 3) {\n        this.page = this.page - 4;\n      } else {}\n    },\n    nextPage() {\n      if (this.page < this.module.length - 4) {\n        this.page = this.page + 4;\n        console.log('add');\n      } else {}\n    },\n    editItem(index) {\n      // Set the edit index for the row\n      this.editIndex = index;\n    },\n    saveEdit(index) {\n      // Save the edited values and reset the edit index\n      this.editIndex = null;\n      console.log('Saved:', this.modalModule[index]);\n    },\n    handleAddPart() {}\n  }\n};","map":{"version":3,"names":["axios","ref","modal","edit","del","components","name","data","module","selectedModuleType","selectedExporter","selectedDate","Date","page","modalModule","modalMod","editIndex","mounted","get","then","response","console","log","filter","exporter_name","length","computed","filteredModule","matchModuleType","module_type","matchExporter","mts","mtd","date_receive","mtn","matchMonth","methods","getModuleStatus","parts","hasNullTapping","some","part","date_tapping","handleModal","id","_id","module_no","value","handleEdit","handleDel","prevPage","nextPage","editItem","index","saveEdit","handleAddPart"],"sources":["C:\\Users\\andhi\\webDev\\realtime stock\\15\\FE\\src\\views\\Unpacking.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <!-- <div class=\"bg\"> -->\r\n    <div class=\"bg head-module\">\r\n      <div\r\n        class=\"head-module\"\r\n        style=\"font-size: 1.5rem; background-color: gray\"\r\n      >\r\n        Information\r\n      </div>\r\n      <div class=\"row m-1 mb-2\">\r\n        <div class=\"data-uploaded mb-3 p-0\">\r\n          <div class=\"filter-section\">\r\n            <label for=\"filter-date\">Production Date</label>\r\n            <input v-model=\"selectedDate\" type=\"date\" id=\"filter-unique\" />\r\n          </div>\r\n\r\n          <div class=\"filter-section\">\r\n            <label for=\"filter-date\">Shift</label>\r\n            <select v-model=\"selectedShift\" name=\"module\" id=\"module\">\r\n              <option value=\"All\">All</option>\r\n              <option value=\"Day Shift\">Day Shift</option>\r\n              <option value=\"Night Shift\">Night Shift</option>\r\n            </select>\r\n          </div>\r\n\r\n          <div class=\"filter-section\">\r\n            <label for=\"filter-date\">Group</label>\r\n            <select v-model=\"selectedGroup\" name=\"module\" id=\"module\">\r\n              <option value=\"All\">All</option>\r\n              <option value=\"Red\">Red</option>\r\n              <option value=\"White\">White</option>\r\n            </select>\r\n          </div>\r\n\r\n          <button type=\"button\" class=\"btn btn-primary\" @click=\"submitForm\">\r\n            Submit\r\n          </button>\r\n        </div>\r\n\r\n        <div class=\"col-4\">\r\n          <div style=\"overflow: hidden; border-radius: 5px\">\r\n            <div class=\"row mdl\">\r\n              <h1 class=\"card-title col-6 col-module bg-info m-0 pt-2\">\r\n                Module Unpacking\r\n              </h1>\r\n              <h1 class=\"card-text col-6 text-center fz1 bg-light\">4</h1>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"col-4\">\r\n          <div style=\"overflow: hidden; border-radius: 5px\">\r\n            <div class=\"row mdl\">\r\n              <h1 class=\"card-title col-6 col-module bg-info m-0 pt-2\">\r\n                Module Tapping\r\n              </h1>\r\n              <h1 class=\"card-text col-6 text-center fz1 bg-light\">8</h1>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"col-4\">\r\n          <div style=\"overflow: hidden; border-radius: 5px\">\r\n            <div class=\"row mdl\">\r\n              <h1 class=\"card-title col-6 col-module bg-info m-0 pt-2\">\r\n                O/F Tapping\r\n              </h1>\r\n              <h1 class=\"card-text col-6 text-center fz1 bg-light\">2</h1>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n\r\n  <div class=\"bg head-module mt-2\">\r\n      <div\r\n        class=\"head-module\"\r\n        style=\"font-size: 1.5rem; background-color: gray\"\r\n      >Data Unpacking</div>\r\n    <table class=\"data-table\">\r\n      <thead>\r\n        <tr>\r\n          <th>No</th>\r\n          <th>Module Type</th>\r\n          <th>Module process</th>\r\n          <th>Exp Name</th>\r\n          <th>Module No.</th>\r\n          <th>Module Type</th>\r\n          <th>Date Receive</th>\r\n          <th>Action</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody\r\n        v-for=\"(module, index) in module.slice(page, page + 4)\"\r\n        :key=\"index\"\r\n      >\r\n      <!-- {{ console.log(module) }} -->\r\n        <tr>\r\n          <td>{{ index + page + 1 }}</td>\r\n          <td>{{ module.module_type }}</td>\r\n          <td >{{ getModuleStatus(module.parts) }}</td>\r\n          <td>{{ module.exporter_name }}</td>\r\n          <td>{{ module.module_no }}</td>\r\n          <td>{{ module.module_note }}</td>\r\n          <td>\r\n            {{\r\n              new Date(module.date_receive).toLocaleDateString(['ban', 'id'])\r\n            }}\r\n          </td>\r\n          <td>\r\n            <button\r\n              type=\"button\"\r\n              class=\"btn btn-secondary\"\r\n              @click=\"handleModal(module._id)\"\r\n            >\r\n              Detail\r\n            </button>\r\n            <button\r\n              type=\"button\"\r\n              class=\"btn btn-primary\"\r\n              @click=\"handleDelete(index)\"\r\n            >\r\n              Delete\r\n            </button>\r\n          </td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n\r\n    <CPagination align=\"center\" aria-label=\"Page navigation example\">\r\n      <CPaginationItem @Click=\"prevPage()\">Previous</CPaginationItem>\r\n      <!-- <CPaginationItem href=\"#\">1</CPaginationItem>\r\n      <CPaginationItem href=\"#\">2</CPaginationItem>\r\n      <CPaginationItem href=\"#\">3</CPaginationItem> -->\r\n      <CPaginationItem @Click=\"nextPage()\">Next</CPaginationItem>\r\n    </CPagination>\r\n\r\n\r\n  </div>\r\n    <!-- !modal       -->\r\n    <CModal\r\n      size=\"xl\"\r\n      :visible=\"modal\"\r\n      @close=\"\r\n        () => {\r\n          modal = false\r\n        }\r\n      \"\r\n    >\r\n      <CModalHeader>\r\n        <CModalTitle id=\"OptionalSizesExample1\"\r\n          >Content List Module {{ modalMod }}</CModalTitle\r\n        >\r\n      </CModalHeader>\r\n      <CModalBody class=\"row\" >\r\n        <button type=\"button\" class=\"btn btn-success justify-content-end col-1\" @click=\"handleAdd()\">\r\n          Add\r\n        </button>\r\n        <div class=\"table-section\">\r\n          <table class=\"data-table\">\r\n            <thead>\r\n              <tr>\r\n                <th>No</th>\r\n                <th>Module No</th>\r\n                <th>Part No</th>\r\n                <th>Part Name</th>\r\n                <th>Unique No</th>\r\n                <th>Qty Kanban</th>\r\n                <th>Qty Remain</th>\r\n                <th>Date Tapping</th>\r\n                <th>Date Overflow</th>\r\n                <th>Date Unpacking</th>\r\n              </tr>\r\n            </thead>\r\n            <tbody>\r\n              <!-- <tr v-if=\"edit == true && del == false\">\r\n                <td><input type=\"checkbox\" style=\"width: 25px\" /></td>\r\n                <td>BA3032</td>\r\n                <td>32111-52110-00</td>\r\n                <td>Plate,Pump</td>\r\n                <td>T105</td>\r\n\r\n                <td><input type=\"text\" value=\"1\" /></td>\r\n                <td><input type=\"text\" value=\"1\" /></td>\r\n                <td><input type=\"date\" value=\"2023-01-01\" /></td>\r\n                <td><input type=\"date\" value=\"2023-01-01\" /></td>\r\n                <td><input type=\"date\" value=\"2023-01-01\" /></td>\r\n              </tr>\r\n              <tr v-if=\"del == true && edit == false\">\r\n                <td><input type=\"checkbox\" style=\"width: 25px\" /></td>\r\n                <td>BA3032</td>\r\n                <td>32111-52110-00</td>\r\n                <td>Plate,Pump</td>\r\n                <td>T105</td>\r\n\r\n                <td>1</td>\r\n                <td>1</td>\r\n                <td>2023-01-01</td>\r\n                <td>2023-01-01</td>\r\n                <td>2023-01-01</td>\r\n              </tr> -->\r\n              <tr\r\n                v-if=\"edit == false && del == false\"\r\n                v-for=\"(module, index) in modalModule\"\r\n              >\r\n                <td>{{ index + 1 }}</td>\r\n                <td>{{ modalMod }}</td>\r\n                <td>{{ module.part_no }}</td>\r\n                <td>{{ module.part_name }}</td>\r\n                <td>{{ module.unique_no }}</td>\r\n\r\n                <td>\r\n                  <input\r\n                    v-if=\"editIndex === index\"\r\n                    v-model=\"module.qty_kanban\"\r\n                  />\r\n                  <span v-else>{{ module.qty_kanban }}</span>\r\n                </td>\r\n                <td>\r\n                  <input\r\n                    v-if=\"editIndex === index\"\r\n                    v-model=\"module.qty_remain\"\r\n                  />\r\n                  <span v-else>{{ module.qty_remain }}</span>\r\n                </td>\r\n                <td>\r\n                  <input\r\n                    v-if=\"editIndex === index\"\r\n                    v-model=\"module.date_tapping\"\r\n                    type=\"date\"\r\n                  />\r\n                  <span v-else>{{\r\n                    new Date(module.date_tapping).toLocaleDateString('id-ID')\r\n                  }}</span>\r\n                </td>\r\n                <td>\r\n                  <input\r\n                    v-if=\"editIndex === index\"\r\n                    v-model=\"module.date_overflow\"\r\n                    type=\"date\"\r\n                  />\r\n                  <span v-else>{{\r\n                    new Date(module.date_overflow).toLocaleDateString('id-ID')\r\n                  }}</span>\r\n                </td>\r\n                <td>\r\n                  <input\r\n                    v-if=\"editIndex === index\"\r\n                    v-model=\"module.date_unpacking\"\r\n                    type=\"date\"\r\n                  />\r\n                  <span v-else>{{\r\n                    new Date(module.date_unpacking).toLocaleDateString('id-ID')\r\n                  }}</span>\r\n                </td>\r\n                <td>\r\n                  <button\r\n                    type=\"button\"\r\n                    class=\"btn btn-warning\"\r\n                    @click=\"handleEdit()\"\r\n                  >\r\n                    Edit\r\n                  </button>\r\n\r\n                  <button\r\n                    type=\"button\"\r\n                    class=\"btn btn-primary\"\r\n                    @click=\"handleDel(index)\"\r\n                  >\r\n                    Del\r\n                  </button>\r\n                </td>\r\n              </tr>\r\n              <tr>\r\n                <!-- {{console.log(modalModule)}} -->\r\n\r\n\r\n                   <td></td>\r\n                   <td>{{modalMod}}</td>\r\n                <td><input type=\"text\" v-model=\"part_no\"></td>\r\n                <td><input type=\"text\" v-model=\"part_name\"></td>\r\n                <td><input type=\"text\" v-model=\"unique_no\"></td>\r\n                \r\n                <td><input type=\"text\" v-model=\"qty_kanban\"></td>\r\n                <td><input type=\"text\" v-model=\"qty_remain\"></td>\r\n                <td><input type=\"date\" v-model=\"date_tapping\"></td>\r\n                <td><input type=\"date\" v-model=\"date_overflow\"></td>\r\n                <td><input type=\"date\" v-model=\"date_unpacking\"></td>\r\n                <button type=\"button\" class=\"btn btn-success\" @click=\"handleAddPart()\">submit</button>\r\n\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n          <div\r\n            v-if=\"edit == true && del == false\"\r\n            class=\"d-flex justify-content-end table-section\"\r\n          >\r\n            <button type=\"button\" class=\"btn btn-success\" @click=\"handleEdit()\">\r\n              Save\r\n            </button>\r\n          </div>\r\n          <div\r\n            v-if=\"del == true && edit == false\"\r\n            class=\"d-flex justify-content-end table-section\"\r\n          >\r\n            <button\r\n              type=\"button\"\r\n              class=\"btn btn-warning\"\r\n              @click=\"handleDel(index)\"\r\n            >\r\n              Delete\r\n            </button>\r\n          </div>\r\n          <div\r\n            v-if=\"edit == false && del == false\"\r\n            class=\"d-flex justify-content-end table-section\"\r\n          >\r\n            <button type=\"button\" class=\"btn btn-warning\" @click=\"handleEdit()\">\r\n              Edit\r\n            </button>\r\n\r\n            <button\r\n              type=\"button\"\r\n              class=\"btn btn-primary\"\r\n              @click=\"handleDel(index)\"\r\n            >\r\n              Delete\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </CModalBody>\r\n    </CModal>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nimport { ref } from 'vue'\r\nconst modal = ref(false)\r\nconst edit = ref(false)\r\nconst del = ref(false)\r\nexport default {\r\n  components: {\r\n    name: 'TappingModule',\r\n  },\r\n  data() {\r\n    return {\r\n      modal,\r\n      edit,\r\n      del,\r\n      module: [],\r\n      selectedModuleType: 'All',\r\n      selectedExporter: 'All',\r\n      selectedDate: new Date(),\r\n\r\n      \r\n      page: 0,\r\n      modalModule: [],\r\n      modalMod: 0,\r\n      editIndex: null,\r\n    }\r\n  },\r\n  mounted() {\r\n    axios.get('http://localhost:5000/module').then((response) => {\r\n      console.log(response.data.data)\r\n      this.module = response.data.data\r\n      // this.tmc = response.data.data.filter(\r\n      //   (module) => module.exporter_name == 'TMC',\r\n      // )\r\n      console.log(\r\n        response.data.data.filter((module) => module.exporter_name == 'TMC')\r\n          .length,\r\n      )\r\n    })\r\n  },\r\n  computed: {\r\n    // Filtered data based on selected filters\r\n    filteredModule() {\r\n      return this.module.filter((module) => {\r\n        // Filter by Module Type\r\n        const matchModuleType =\r\n          this.selectedModuleType === 'All' ||\r\n          module.module_type === this.selectedModuleType\r\n\r\n        // Filter by Exporter Name\r\n        console.log(this.selectedExporter)\r\n        const matchExporter =\r\n          this.selectedExporter === 'All' ||\r\n          module.exporter_name === this.selectedExporter\r\n\r\n        // Filter by Month Receive\r\n        let mts = new Date(this.selectedDate)\r\n        let mtd = new Date(module.date_receive)\r\n        let mtn = new Date()\r\n        console.log(mtn)\r\n     