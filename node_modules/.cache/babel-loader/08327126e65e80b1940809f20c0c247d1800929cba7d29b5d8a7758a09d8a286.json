8, value: '27,319' },\n    ]\n    const tableExample = [\n      \n    ]\n\n    return {\n      tableExample,\n      progressGroupExample1,\n      progressGroupExample2,\n      progressGroupExample3,\n    }\n  },\n  methods: {\n    toggleFullScreen() {\n      const doc = document.documentElement;\n      if (!document.fullscreenElement) {\n        doc.requestFullscreen().catch((err) => {\n          alert(\n            `Error attempting to enable full-screen mode: ${err.message} (${err.name} )`,\n          )\n        })\n      } else {\n        const currentHeight = parseInt(this.heightValue);\n        this.heightValue = `100vh`;\n      }\n    },\n\n    checkFullScreen() {\n      this.isFullScreen = !document.fullscreenElement\n    },\n  },\n  beforeDestroy() {\n    document.removeEventListener('fullscreenchange', this.checkFullScreen)\n  },\n  }\n\n</script>\n"],"mappings":"AAuFA,OAAOA,gBAAe,MAAO,2BAA0B;AACvD,OAAOC,aAAY,MAAO,iCAAgC;AAC1D,OAAOC,aAAY,MAAO,iCAAgC;AAC1D,OAAOC,OAAM,MAAO,0BAAyB;AAC7C,OAAOC,IAAG,MAAO,kCAAkC;AAEnD,eAAe;EAEbC,IAAI,EAAE,WAAW;EACjBC,UAAU,EAAE;IACZH,OAAO;IAACC;EACR,CAAC;EAACA,IAAIA,CAAA,EAAG;IACP,OAAO;MACLG,YAAY,EAAE,KAAK;MACpBC,WAAW,EAAE,MAAM;MAClBC,KAAK,EAAEL,IAAI,CAACM,KAAI,CACbC,MAAM,CAAEC,IAAI,IAAKA,IAAI,CAACC,WAAU,GAAI,GAAG,EACvCC,GAAG,CAAEF,IAAI,IAAKA,IAAI,EAClBG,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAKD,CAAC,CAACH,WAAU,GAAII,CAAC,CAACJ,WAAW;IACjD;EACF,CAAC;EAAGK,OAAOA,CAAA,EAAG;IACZ;;IAEA;IACAC,QAAQ,CAACC,gBAAgB,CAAC,kBAAkB,EAAE,IAAI,CAACC,eAAe;EACpE,CAAC;EACDC,KAAKA,CAAA,EAAG;IACN,MAAMC,qBAAoB,GAAI,CAC5B;MAAEC,KAAK,EAAE,QAAQ;MAAEC,MAAM,EAAE,EAAE;MAAEC,MAAM,EAAE;IAAG,CAAC,EAC3C;MAAEF,KAAK,EAAE,SAAS;MAAEC,MAAM,EAAE,EAAE;MAAEC,MAAM,EAAE;IAAG,CAAC,EAC5C;MAAEF,KAAK,EAAE,WAAW;MAAEC,MAAM,EAAE,EAAE;MAAEC,MAAM,EAAE;IAAG,CAAC,EAC9C;MAAEF,KAAK,EAAE,UAAU;MAAEC,MAAM,EAAE,EAAE;MAAEC,MAAM,EAAE;IAAG,CAAC,EAC7C;MAAEF,KAAK,EAAE,QAAQ;MAAEC,MAAM,EAAE,EAAE;MAAEC,MAAM,EAAE;IAAG,CAAC,EAC3C;MAAEF,KAAK,EAAE,UAAU;MAAEC,MAAM,EAAE,EAAE;MAAEC,MAAM,EAAE;IAAG,CAAC,EAC7C;M