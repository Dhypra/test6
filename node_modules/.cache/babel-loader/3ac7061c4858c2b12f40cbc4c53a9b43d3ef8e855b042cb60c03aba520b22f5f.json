 = this.module.filter(module => module._id == id)[0].module_no;\n      this.modalModule = this.module.filter(module => module._id == id)[0].parts;\n      //  console.log(this.module.filter((module) => module._id == id)[0].parts)\n      this.modal = !modal.value;\n      this.edit = false;\n      this.del = false;\n    },\n    handleEdit() {\n      this.edit = !edit.value;\n    },\n    handleDel() {\n      this.del = !del.value;\n    },\n    prevPage() {\n      if (this.page > 3) {\n        this.page = this.page - 4;\n      } else {}\n    },\n    nextPage() {\n      if (this.page < this.filteredModule.length - 4) {\n        this.page = this.page + 4;\n        console.log('add');\n      } else {}\n    },\n    editItem(index) {\n      // Set the edit index for the row\n      this.editIndex = index;\n    },\n    saveEdit(index) {\n      // Save the edited values and reset the edit index\n      this.editIndex = null;\n      console.log('Saved:', this.modalModule[index]);\n    }\n  }\n};","map":{"version":3,"names":["axios","ref","modal","edit","del","components","name","data","module","selectedModuleType","selectedExporter","selectedMonth","Date","page","modalModule","modalMod","editIndex","mounted","get","then","response","console","log","filter","exporter_name","length","computed","filteredModule","matchModuleType","module_type","matchExporter","mts","getMonth","mtd","date_receive","mtn","matchMonth","methods","handleModal","id","_id","module_no","parts","value","handleEdit","handleDel","prevPage","nextPage","editItem","index","saveEdit"],"sources":["C:\\Users\\andhi\\webDev\\realtime stock\\14\\FE\\src\\views\\TappingModule.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <div class=\"card-body\">\r\n      <div class=\"row\" style=\"padding: 10px\">\r\n        <div class=\"col-md-3\">\r\n          <div style=\"overflow: hidden; border-radius: 5px\">\r\n            <div class=\"text-center row mdl\">\r\n              <h1 class=\"card-title col-6